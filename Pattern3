public class Pattern3 {
    public static void main(String[] args){
        int i, j, k;  
        int m= 1;
         
        for (i = 1; i <= 4; i++)   
        {  
            for (j = i; j < 4; j++)  
            {  
                System.out.print(" ");   
            }  
            for (k = 1; k <= i; k++)  
            {  
                System.out.print(m);
                m++; 
            }  
            System.out.println();   
        }  
             
    }  
}
